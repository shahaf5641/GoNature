<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>

<HBox alignment="TOP_RIGHT" prefHeight="134.0" prefWidth="803.0" styleClass="toolbar-container" stylesheets="@../resources/G21CSS.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.HomeButtonsController">
    <children>
        <Button fx:id="orderVisit" onMouseClicked="#loadOrderVisit" prefHeight="400.0" prefWidth="160.0" styleClass="toolbar-container">
            <graphic>
                <Text fx:id="MenuButtons" strokeLineCap="BUTT" text="Order A Visit" />
            </graphic>
        </Button>
        <Button fx:id="travelerLogin" onMouseClicked="#loadTravelerLogin" prefHeight="400.0" prefWidth="160.0" styleClass="toolbar-container">
            <graphic>
                <Text fx:id="MenuButtons" strokeLineCap="BUTT" text="Traveler Login" />
            </graphic>
        </Button>
        <Button fx:id="employeeLogin" onMouseClicked="#loadEmployeeLogin" prefHeight="400.0" prefWidth="160.0" styleClass="toolbar-container">
            <graphic>
                <Text fx:id="MenuButtons" strokeLineCap="BUTT" text="Employee Login" />
            </graphic>
        </Button>
        <Button fx:id="showPrices" onMouseClicked="#loadPrices" prefHeight="400.0" prefWidth="160.0" styleClass="toolbar-container">
            <graphic>
                <Text fx:id="MenuButtons" strokeLineCap="BUTT" text="Prices" />
            </graphic>
        </Button>
        <Button fx:id="contactUs" onMouseClicked="#loadContactUs" prefHeight="400.0" prefWidth="160.0" styleClass="toolbar-container">
            <graphic>
                <Text fx:id="MenuButtons" strokeLineCap="BUTT" text="Contact Us" />
            </graphic>
        </Button>
    </children>
</HBox>
